apiVersion: v1
kind: Config
current-context: dev
preferences: {}
contexts:
- context:
    cluster: dev
    user: kubernetes-admin
  name: dev
- context:
    cluster: bare-metal
    user: bare-metal-admin
  name: bare-metal
- context:
    cluster: bare-metal-master
    user: bare-metal-master-admin
  name: bare-metal-master
clusters:
- name: dev
  cluster:
    certificate-authority-data: Base64CertificateHere==
    server: https://127.0.0.1:6445
- name: bare-metal
  cluster:
    certificate-authority-data: Base64CertificateHere==
    server: https://127.0.0.1:6445
- name: bare-metal-master
  cluster:
    certificate-authority-data: Base64CertificateHere==
    server: https://10.10.10.80:6443
users:
- name: kubernetes-admin
  user:
    client-certificate-data: Base64CertificateHere==
    client-key-data: Base64KeyHere
- name: bare-metal-admin
  user:
    client-certificate-data: Base64CertificateHere==
    client-key-data: Base64KeyHere==
- name: bare-metal-master-admin
  user:
    token: RS256-JWT_Token-Here
